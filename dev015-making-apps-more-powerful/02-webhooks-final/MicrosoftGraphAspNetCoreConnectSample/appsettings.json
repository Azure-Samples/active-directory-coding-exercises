{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "CallbackPath": "/signin-oidc",

    "BaseUrl": "ENTER_YOUR_APP_URL", // e.g. https://YOURWEBAPP.azurewebsites.net
    "ClientId": "ENTER_YOUR_APP_ID ",
    "ClientSecret": "ENTER_YOUR_SECRET", // This sample uses a password (secret) to authenticate. Production apps should use a certificate.
    "NotificationUrl": "ENTER_YOUR_NOTIFYURL/notification/listen", // e.g. https://YOURWEBAPP.azurewebsites.net/notification/listen
    "Scopes": "openid email profile offline_access",
    "GraphResourceId": "https://graph.microsoft.com/",
    "GraphScopes": "User.Read User.ReadBasic.All Mail.Send"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}